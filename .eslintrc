{
  "parser": "babel-eslint",
  "plugins": ["require-in-package"],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "brace-style": [2, "1tbs"],
    "camelcase": [2, {"properties": "always"}],
    "comma-dangle": [1, "always-multiline"],
    "comma-style": [2, "last"],
    "default-case": 2,
    "dot-location": [2, "property"],
    "eqeqeq": [2, "allow-null"],
    "func-style": 0,
    "generator-star-spacing": [2, "after"],
    "guard-for-in": 2,
    "handle-callback-err": 0,
    "id-blacklist": [2, "data", "cb", "callback", "params", "val", "list", "values", "promise", "tmp", "temp", "ary", "array", "count", "num", "string"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "jsx-quotes": [2, "prefer-double"],
    "keyword-spacing": 1,
    "max-params": ["error", 3],
    "new-cap": [2, {"capIsNew": false}],
    "no-alert": 2,
    "no-bitwise": 2,
    "no-cond-assign": [2, "always"],
    "no-constant-condition": 0,
    "no-console": 1,
    "no-const-assign": 2,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-eval": 2,
    "no-extra-semi": 1,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-floating-decimal": 2,
    "no-nested-ternary": 2,
    "no-path-concat": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [1, "nofunc"],
    "no-useless-call": 2,
    "no-unreachable": 2,
    "no-var": 2,
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quotes": [1, "double", "avoid-escape"],
    "radix": 2,
    "require-in-package/require-in-package": 2,
    "semi": [1, "never"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "use-isnan": 2,
    "valid-jsdoc": [2, {"requireReturn": false, "requireReturnDescription": false}],
    "valid-typeof": 2,
    "wrap-iife": 2
  },
  "globals": {
    "fetch": true
  }
}
